<mvc:View controllerName="devicerecognition.controller.Vorrat_Loschen"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}" titleAlignment="Center" navButtonPress="onBack"
        showNavButton="false" class="pageCls sapUiSizeCompact">
        <content>
            <!-- <VBox id="vBoxId1" alignContent="Stretch" alignItems="Center"
                class="sapUiSmallMarginTop"> -->
                <!-- <Title id="vorratTitleId" text="{i18n>subTitleThree}"
                class="sapUiSmallMarginTop"></Title> -->
                <Title id="pageSubTitle" text="{i18n>subTitleThree}" class="hdrclass"/>
                <RadioButtonGroup id="radioButtonGroup1" columns="2" width="100%"
                    select=".changeRegion">
                    <RadioButton id="radioButtonOneId1" text="{i18n>clearStock}" selected="true"
                        class="sapUiSmallMarginEnd" />
                    <RadioButton id="radioButtonTwoId1" text="{i18n>deleteDevices}"
                        class="sapUiSmallMarginBeginEnd" />
                </RadioButtonGroup>
                <HBox id="hBoxOne" justifyContent="SpaceBetween">
                <Toolbar id="toolIdOne">
                    <Button id="backId1" type="Emphasized" text="{i18n>back}"
                        class="spaceBackBtnCls" press="onBack" />
                    <OverflowToolbar id="spaceIdOne" />
                    <Button id="clearStockId1" type="Emphasized" text="{i18n>deleteQueue}"
                        class="spaceBtnCls" press="onQueueClearance" ariaHasPopup="Dialog" />
                        <!-- <ToolbarSpacer id="spaceIdTwo" /> -->
                    <Button id="forwardId1" type="Emphasized" text="{i18n>continue}" visible="false"
                        class="spaceCtnBtnCls" press="onContinue" />
                </Toolbar>
                </HBox>
                <Text id="propertyTextId" visible="false"/>
                <!-- <Button id="check" text="check" press="onPress"/> -->
                <!-- <Text id="myId" text="{tplnr}"/> -->
            <!-- </VBox> -->
        </content>
    </Page>
</mvc:View>