<mvc:View controllerName="devicerecognition.controller.Property_Selection"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns:l="sap.ui.layout"
	xmlns:w="sap.ui.webc.main"
	xmlns:dr="sap.ui.webc.main"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page id="propSelPage" title="{i18n>title}" class="pageCls sapUiSizeCompact">
		<customHeader>
			<Bar id="PSBarIdOne">
				<contentMiddle>
					<Title id="PSHeaderTiltle" class="hdrclass" text="{i18n>title}" />
				</contentMiddle>
			</Bar>
		</customHeader>
		<content>
			<!-- <Title id="VLPageSubTitle" text="{i18n>propSelection}" class="tableTitleCls" /> -->
			<f:SimpleForm id="SimpleFormDisplay354" class=".urGrpTtlWeb1" editable="true"
				layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4"
				labelSpanS="12" adjustLabelSpan="false" emptySpanXL="2" emptySpanL="2"
				emptySpanM="2" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2"
				ariaLabelledBy="Title1" singleContainerFullSize="false">
				<f:toolbar>
					<Toolbar id="PSToolBarIdOne">
						<Title id="PSTitleOne" titleStyle="H5"
							text="{i18n>psSubTitle}" class="formHeaderCls"/>
					</Toolbar>
				</f:toolbar>
				<f:content>
					<Label id="PSLabelIdOne" text="{i18n>PropNum}" design="Bold" />
					<HBox id="PSHBoxIdOne">
						<Input id="PSTplnrInput" liveChange="onchangeTplnr" submit="onSubmitTplnr" />
						<Text id="PSTplnrTxt" visible="false" />
						<Button id="PSsearchBtn" class="psSearchBtnCls" icon="sap-icon://search"
							type="Emphasized" press="onSearch" width="10%" />
					</HBox>
				</f:content>
			</f:SimpleForm>
			<Toolbar id="PSToolBarIdTwo">
				<Button text="{i18n>back}" enabled="false" visible="false" id="backBtn"
					class="contBtnCls"
					icon="sap-icon://close-command-field" type="Emphasized" press="onBack"></Button>
				<ToolbarSpacer id="PSToolBarSpacerId" />
				<Button id="PSCtnButtonId" text="{i18n>continue}" iconFirst="false"
					class="contBtnCls"
					icon="sap-icon://open-command-field" type="Emphasized" press="onContinue"></Button>
			</Toolbar>
		</content>
	</Page>
</mvc:View>