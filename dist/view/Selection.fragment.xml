<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog id="helpDialogId"
		title="{i18n>dialogTitle}"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		supportRanges="false"
        supportMultiselect="false"
		key="ProductCode"
		descriptionKey="ProductName">
		<filterBar>
			<fb:FilterBar id="filterBarOne" advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem id="filterGroupIdOne" 
						groupName="__$INTERNAL$"
						name="tplnr"
						label="{i18n>FunLoc}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="tplnr" id="tplnrId"/>
						</fb:control>
					</fb:FilterGroupItem>
                    <fb:FilterGroupItem id="filterGroupIdTwo"
						groupName="__$INTERNAL$"
						name="street"
						label="{i18n>Street}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input id="streetId" name="street" />
						</fb:control>
					</fb:FilterGroupItem>
                    <fb:FilterGroupItem id="filterGroupIdThree"
						groupName="__$INTERNAL$"
						name="houseNum"
						label="{i18n>HsNum}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="houseNum" id="houseId"/>
						</fb:control>
					</fb:FilterGroupItem>
                     <fb:FilterGroupItem id="filterGroupIdFour"
						groupName="__$INTERNAL$"
						name="plz"
						label="{i18n>plz}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="plz" id="plzId"/>
						</fb:control>
					</fb:FilterGroupItem>
                    <fb:FilterGroupItem id="filterGroupIdFive"
						groupName="__$INTERNAL$"
						name="Ort"
						label="{i18n>Ort}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Ort" id="ortId"/>
						</fb:control>
					</fb:FilterGroupItem>
                    <fb:FilterGroupItem id="filterGroupIdSix"
						groupName="__$INTERNAL$"
						name="ObjectNumber"
						label="{i18n>ObjNum}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input id="objectNumId" name="ObjectNumber" />
						</fb:control>
					</fb:FilterGroupItem>
                    <fb:FilterGroupItem id="filterGroupIdSeven"
						groupName="__$INTERNAL$"
						name="parnr"
						label="{i18n>ContactPerson}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input id="parnrId" name="parnr" />
						</fb:control>
					</fb:FilterGroupItem>

				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>

